{
    "name": "yiisoft/factory",
    "type": "library",
    "description": "Yii Factory",
    "keywords": [
        "factory"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/factory/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/factory"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "psr/container": "1.0.0"
    },
    "require-dev": {
        "infection/infection": "^0.16.3",
        "league/container": "^3.2",
        "phan/phan": "^3.0",
        "phpunit/phpunit": "^9.3",
        "yiisoft/injector": "^1.0@dev",
        "yiisoft/di": "^3.0@dev"
    },
    "suggest": {
        "yiisoft/injector": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Factory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Factory\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
